$(function(){$(".ownershiptype").click(function(){$(this).addClass("active"),$(this).siblings("div:first").removeClass("active");var n=$(this).html().toLowerCase().trim()=="owned";$("#OwnerShipType").val(n?ownerShipMaster.Owned:ownerShipMaster.Rented),n?($(".propdiv").show(),$(".propage").show()):($(".propdiv").hide(),$(".propage").hide(),$("#BuildingValue").val(""),$("#BuildingAge").val(1))}),onwershipType&&onwershipType>0&&(onwershipType==ownerShipMaster.Owned?($("#OwnerShipType").val(ownerShipMaster.Owned),$(".owned").addClass("active"),$(".rented").removeClass("active"),$(".propdiv").show(),$(".propage").show()):($("#OwnerShipType").val(ownerShipMaster.Rented),$(".owned").removeClass("active"),$(".rented").addClass("active"),$(".propdiv").hide(),$(".propage").hide(),$("#BuildingValue").val(""),$("#BuildingAge").val(1)));$("#BuildingValue, #ContentValue").on("keyup blur",function(){$("#BuildingValue").val()>0||$("#ContentValue").val()>0?$(".buildingcontenterror").hide():$(".buildingcontenterror").show()});$("#formprequote1").submit(function(){var n=parseInt($("#BuildingValue").val()),t=parseInt($("#ContentValue").val()),i;if(n>0||t>0){if(n&&n>0&&(i=checkifNumberisWholeOr50KMultiple(n),!i))return $(".buildingvalerror").html(""),$(".buildingvalerror").show(),$(".buildingvalerror").html("Value should be a whole no or multiple of 50K"),!1;if(t&&t>0&&(i=checkifNumberisWholeOr50KMultiple(t),!i))return $(".contentvalerror").html(""),$(".contentvalerror").show(),$(".contentvalerror").html("Value should be a whole no or multiple of 50K"),!1}else return $(".buildingcontenterror").show(),!1}),$(".footer").addClass("fotter_text2")})