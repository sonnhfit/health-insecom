$(function(){var n=$("#citylist");n.typeahead({minLength:3,source:function(t,i){return $.getJSON(n.data("url"),{prefix:t},function(n){var t=[];return $.each(n,function(n,i){t.push({id:i.CityId+"|"+i.StateId,name:i.CityName})}),i(t)})}}),n.change(function(){var t=n.typeahead("getActive");t&&($("#CityID").val(t.id.split("|")[0]),$("#StateID").val(t.id.split("|")[1]))}),$("#pq2submitbutton").click(function(n){($("#CityID").val()=="0"||$("#StateID").val()=="0")&&($("#citylist").val(""),$("#citylist").text(""),n.preventDefault())}),$(".footer").addClass("fotter_text2")})